{
  "private": true,
  "displayName": "Homebridge Honeywell Home",
  "name": "homebridge-honeywell-home",
  "version": "6.0.0",
  "description": "A Honeywell Home https://honeywellhome.com plugin for Homebridge: https://github.com/homebridge/homebridge",
  "author": "donavanbecker",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/donavanbecker/homebridge-honeywell-thermostat.git"
  },
  "bugs": {
    "url": "https://github.com/donavanbecker/homebridge-honeywell-thermostat/issues"
  },
  "engines": {
    "node": "^10.17.0",
    "homebridge": "^1.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "developer": "DEBUG=honeywell-home homebridge -D -U `pwd` -P `pwd`",
    "install": "sudo npm install && npm audit",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "homebridge-plugin",
    "honeywell",
    "honeywell-home",
    "thermostat"
  ],
  "dependencies": {
    "@types/qs": "^6.9.3",
    "axios": "^0.19.2",
    "got": "^11.5.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "rxjs": "^6.6.0",
    "superagent": "^5.3.1"
  },
  "devDependencies": {
    "typescript-axios-wb": "^1.0.3",
    "@types/node": "^14.0.14",
    "@types/got": "^9.6.11",
    "@types/superagent": "^4.1.8",
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "homebridge": "^1.1.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
