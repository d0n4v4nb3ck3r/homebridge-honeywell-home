{
  "displayName": "Resideo",
  "name": "homebridge-resideo",
  "version": "24.1.0",
  "description": "The [Homebridge](https://homebridge.io) Resideo plugin allows you to access your [Resideo](https://resideo.com) device(s) from HomeKit.",
  "author": "donavanbecker",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/donavanbecker/homebridge-resideo.git"
  },
  "bugs": {
    "url": "https://github.com/donavanbecker/homebridge-resideo/issues"
  },
  "engines": {
    "homebridge": "^1.6.1",
    "node": "^18 || ^20"
  },
  "main": "dist/index.js",
  "scripts": {
    "check": "npm install && npm outdated",
    "update": "ncu -u && npm update && npm install",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build",
    "postpublish": "npm run clean",
    "clean": "rimraf ./dist",
    "test": "eslint src/**.ts"
  },
  "funding": [
    {
      "type": "Paypal",
      "url": "https://paypal.me/donavanbecker"
    },
    {
      "type": "GitHub",
      "url": "https://github.com/sponsors/donavanbecker"
    }
  ],
  "keywords": [
    "homebridge-plugin",
    "honeywell",
    "honeywell-home",
    "thermostat",
    "roomsensor",
    "leaksensor",
    "room sensor",
    "leak sensor",
    "lcc",
    "tcc",
    "Resideo",
    "honeywellhome",
    "honeywell home",
    "temperature",
    "humidity",
    "leak"
  ],
  "dependencies": {
    "axios": "1.6.2",
    "rxjs": "^7.8.1",
    "@homebridge/plugin-ui-utils": "^1.0.0",
    "super-stringify": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "homebridge": "^1.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "typescript-axios-wb": "^1.0.3",
    "npm-check-updates": "^16.14.6"
  }
}
